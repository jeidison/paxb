{
    "name": "jeidison/paxb",
    "description": "PHP XML Binding",
    "type": "library",
    "keywords": ["PHP", "XML", "XML Binding", "PAXB"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeidison Farias",
            "email": "jeidison.farias@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.9",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "PhpXml\\PAXB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
